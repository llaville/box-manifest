{
    "name": "bartlett/box-manifest",
    "description": "Creates manifests for a PHP Archive (PHAR) and the BOX project (https://github.com/box-project/box)",
    "type": "project",
    "require": {
        "php": "^8.1",
        "ext-phar": "*",
        "composer/composer": "^2.2",
        "cyclonedx/cyclonedx-library": "^2.1",
        "humbug/box": ">=4.0 <4.4",
        "symfony/filesystem": "^6.1",
        "symfony/runtime": "^6.1",
        "symfony/serializer": "^6.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Bartlett\\BoxManifest\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/unit"
        ]
    },
    "authors": [
        {
            "name": "Laurent Laville"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": [
        "bin/box-manifest"
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/runtime": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        },
        "runtime": {
            "autoload_template": "autoload_phar_runtime.template"
        }
    }
}
